#ifndef __LED_H
#define __LED_H	
#include "stm32f10x.h" 

#define LCD_INT			GPIO_Pin_5
#define LCD_INT_NUM		5
#define LCD_CS			GPIO_Pin_8
#define LCS_CS_NUM		8
#define LCD_WR			GPIO_Pin_14
#define LCD_WR_NUM		14
#define LCD_RS			GPIO_Pin_13
#define LCD_RS_NUM		13
#define LCD_RD			GPIO_Pin_15
#define LCD_RD_NUM		15
#define TP_CS			GPIO_Pin_9
#define TP_CS_NUM		9

void LED_Init(void);
void GPIO_SPI3_Init(void);
#endif


